{
    "manifest_version": 2,

    "name": "Link Grabber",
    "description": "Grabs Links.",
    "version": "1.0",
    "permissions": ["storage"],




    "content_scripts": [
        {
            "matches": ["https://app.pluralsight.com/*"],
            "css": ["style.css"],
            "js": ["jquery-3.2.1.min.js", "grabber.js"]
    }
  ]


}
